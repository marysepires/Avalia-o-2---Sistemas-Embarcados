#include <stdio.h>
#include<stdlib.h>

int main() {
	typedef enum { false, true } bool;
  int i, j;
	bool a, b, x, c;

  printf ("A: ");
  scanf("%d", &i); //entrada de a (0 ou 1)
  a = i;
	printf("B: "); // entrada de b (0 ou 1)
  scanf("%d", &j);
  b = j;

	if (a == true || b == true){
	    x = true;
	}
	else{
	    x = false ;
	}c = !x;

	printf("NOT(A OR B): %d", c);					//saida
}
